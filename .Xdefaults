!! Fonts
Xft.lcdfilter: lcddefault
Xft.antialias: true
Xft.autohint: 0
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.dpi: 96

!! Colors
*foreground: #00cc00
*foreground_bold: #a8a19f
*cursor: #a8a19f
*background: #1b1918
!!*background: rgba(0, 0, 0, .7)
*color0: #1b1918
*color8: #766e6b
*color1: #f22c40
*color9: #f22c40
*color2: #5ab738
*color10: #5ab738
*color3: #d5911a
*color11: #d5911a
*color4: #407ee7
*color12: #407ee7
*color5: #6666ea
*color13: #6666ea
*color6: #00ad9c
*color14: #00ad9c
*color7: #a8a19f
*color15: #f1efee

!! Styling
URxvt.intensityStyles: false
URxvt.background: [70]#000000
URxvt.depth: 32
URxvt.font: xft:monospace:size=11
URxvt.scrollBar: false
URxvt.cursorColor:white
URxvt.colorUL: #4682B4

!! Settings
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.C-semicolon: \033[;

!! Extensions
URxvt.perl-ext-common: default,matcher,resize-font,url-select,keyboard-select,selection-to-clipboard,clipboard
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: firefox-developer-edition
URxvt.url-select.underline: true
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.url-launcher:	firefox-developer-edition
URxvt.matcher.button: 1
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

!! Rofi theme
rofi.color-enabled:	true
rofi.color-window: #000, #000, #000
rofi.color-normal: #111, #819396, #222, #008ed4, #ffffff
rofi.color-active: #002b37, #008ed4, #003643, #008ed4, #66c6ff
rofi.color-urgent: #002b37, #da4281, #003643, #008ed4, #890661
rofi.fake-transparency:	true
rofi.lines:	3
rofi.bw: 0
rofi.opacity: "10"
rofi.hide-scrollbar: true
rofi.width: 30

!! Rofi keybindings
rofi.kb-accept-entry: Return
rofi.kb-remove-to-eol:
rofi.kb-row-down: Ctrl+j
rofi.kb-row-up: Ctrl+k
rofi.terminal: urxvt
