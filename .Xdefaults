!! Styling
URxvt.intensityStyles: false
!!Bug: 32->pixbuf always transparent
URxvt.depth: 24
URxvt.font: xft:FuraCode Nerd Font:Regular:size=11
URxvt.scrollBar: false
URxvt.saveLines: 10000
URxvt.cursorColor: white
URxvt.colorUL: #4682B4

!! Settings
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.C-semicolon: ^[[25~
URxvt.keysym.C-Return: ^[[26~
URxvt.keysym.C-S-Return: ^[[27~
/* URxvt.keysym.C-A-f: ^[[26~ */
URxvt.keysym.C-Up: \033[1;5A
URxvt.keysym.C-Down: \033[1;5B
URxvt.keysym.C-Left: \033[1;5D
URxvt.keysym.C-Right: \033[1;5C
URxvt.keysym.C-1: command:\033]11;#1d2021\007\033]10;#ebdbb2\007
URxvt.keysym.C-2: command:\033]11;#ebdbb2\007\033]10;#1d2021\007

!! Extensions
URxvt.perl-ext-common: default,resize-font,matcher,url-select,keyboard-select,clipboard,tabbedex
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: firefox-developer-edition
URxvt.url-select.underline: true
URxvt.url-launcher: firefox-developer-edition
!!https://www.reddit.com/r/urxvt/comments/4xeoji/clicking_on_url_opens_the_url_twice_or_more/db3fytu/
URxvt.url-select.button: 10
URxvt.matcher.button: 1
URxvt.keysym.C-C: perl:clipboard:copy
URxvt.keysym.C-V: perl:clipboard:paste
URxvt.keysym.C-t: tabbedex:new_tab
URxvt.keysym.C-Tab: tabbedex:next_tab
URxvt.keysym.C-S-ISO_Left_Tab: tabbedex:prev_tab
URxvt.keysym.C-S-Left:  tabbedex:move_tab_left
URxvt.keysym.C-S-Right: tabbedex:move_tab_right
URxvt.tabbedex.no-tabbedex-keys: true
URxvt.keyboard-select.clipboard: true
